{
  "name": "UofT Video Downloader",
  "version": "0.0.1",
  "description": "Downloads lectures hosted by the University of Toronto",
  "permissions": ["storage", "declarativeContent", "webRequest", "downloads", "alarms", "*://*.utoronto.ca/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "48": "images/48x48.png",
      "128": "images/128x128.png"
    }
  },
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "manifest_version": 2
}